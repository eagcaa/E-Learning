@model List<Entity.User>

<h2>Users in Course</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <!-- Diğer kullanıcı özellikleri -->
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserId</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <!-- Diğer kullanıcı özellikleri -->
            </tr>
        }
    </tbody>
</table>
